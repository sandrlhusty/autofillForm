{
  "name": "Nô lệ DRL",
  "version": "1.0.1",
  "manifest_version": 3,
  "action": {
    "default_icon": {
      "16": "images/search16.png",
      "32": "images/search32.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "scripts/content.js"
      ]
    }
  ]
}
